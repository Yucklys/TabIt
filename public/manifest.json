{
  "manifest_version": 3,
  "name": "TabIt",
  "version": "1.0.0",
  "description": "AI Powered Tab Manager for Chrome",
  "action": {
    "default_popup": "index.html",
    "default_title": "TabIt - AI Tab Manager"
  },
  "permissions": [
	"tabs",
	"tabGroups",
	"scripting",
	"storage"
  ],
  "host_permissions": [
	"http://*/*",
	"https://*/*"
  ]
}
